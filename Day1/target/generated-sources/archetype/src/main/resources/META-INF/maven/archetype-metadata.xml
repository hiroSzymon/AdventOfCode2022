<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd" name="Day"
    xmlns="https://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" packaged="true" encoding="UTF-8">
      <directory>src/main/java</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>input.txt</include>
      </includes>
    </fileSet>
  </fileSets>
  <requiredProperties>
    <requiredProperty key="year">
      <defaultValue>2022</defaultValue>
    </requiredProperty>
    <requiredProperty key="groupId">
      <defaultValue>com.hiroszymon</defaultValue>
    </requiredProperty>
    <requiredProperty key="version">
      <defaultValue>1.0-SNAPSHOT:</defaultValue>
    </requiredProperty>
    <requiredProperty key="dayNo"/>
    <requiredProperty key="artifactId">
      <defaultValue>Day${dayNo}</defaultValue>
    </requiredProperty>
    <requiredProperty key="package">
      <defaultValue>com.hiroszymon.aoc2022</defaultValue>
    </requiredProperty>
  </requiredProperties>
</archetype-descriptor>
